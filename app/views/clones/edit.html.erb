
<div class="header">
  
  <div class="img">
    
  </div>
  <div class="menuber">
    
  </div>
 
</div>

<div class="content">
  <div class="side">
    <div class="acount"></div>
    <h3>Tadaka</h3>
    <p>@nozomi_clone</p>
  </div>
  
  <div class="main">

<%= form_with(model: @clone, local: true) do |form| %>
<% if @clone.errors.any? %>
    <div id="error_explanation">
      <h2><%= @clone.errors.count %>件のエラーがあります。</h2>
      <ul>

      <% @clone.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

<br >

  <div class="clone_title">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
<br >
  <%= form.submit %>
<% end %>

</div>
</div>
<div id="footer">
  <p class="footer">@NozomiTakada</p>
</div>